	<item>
				<title>Siyabonga Stig Sesanti</title>
		<link>http://wordpress.local/wp-comics/siyabonga-stig-sesanti/</link>
		<pubDate>Fri, 09 Jan 2015 15:30:21 +0000</pubDate>
		<dc:creator><![CDATA[admin]]></dc:creator>
