	<item>
				<title>Hannes Brummer</title>
		<link>http://wordpress.local/wp-comics/hannes-brummer/</link>
		<pubDate>Fri, 09 Jan 2015 15:30:21 +0000</pubDate>
		<dc:creator><![CDATA[admin]]></dc:creator>
